  go_prev = <Primary><Shift>Tab
  new_terminator = <Alt><Super>t
  new_window = <Alt><Super>t
  resize_down = <Primary><Shift>Down
  resize_up = <Primary><Shift>Up
  split_horiz = <Primary><Alt>Down
  split_vert = <Primary><Alt>Right
[layouts]
